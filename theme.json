{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 3,
    "templateParts": [
		{
			"area": "ariom-header",
			"name": "ariom-header",
			"title": "Header"
		},
		{
			"area": "ariom-footer",
			"name": "ariom-footer",
			"title": "Footer"
		}
	],
    "settings": {
        "color": {
            "customDuotone": false,
            "customGradient": false,
            "defaultDuotone": false,
            "defaultGradients": false,
            "defaultPalette": false,
            "duotone": [],
            "gradients": [],
            "palette": [
                {
                    "slug": "primary",
                    "color": "#f99d2a",
                    "name": "Primary Blue"
                },
                {
                    "slug": "secondary",
                    "color": "#ae6e1d",
                    "name": "Secondary"
                },
                {
                    "slug": "white",
                    "color": "#ffffff",
                    "name": "White"
                },
                {
                    "slug": "black",
                    "color": "#000000",
                    "name": "Black"
                },
                {
                    "slug": "grey",
                    "color": "#848484",
                    "name": "Grey"
                },
                {
                    "slug": "darkgrey",
                    "color": "#424242",
                    "name": "Dark Grey"
                }
            ]
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFace": [
                        {
                            "fontFamily": "Source Sans 3",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "200 900",
                            "src": [
                                "file:./assets/fonts/SourceSans3-VariableFont_wght.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Source Sans 3",
                            "fontStretch": "normal",
                            "fontStyle": "italic",
                            "fontWeight": "200 900",
                            "src": [
                                "file:./assets/fonts/SourceSans3-Italic-VariableFont_wght.ttf"
                            ]
                        }
                    ],
                    "fontFamily": "\"Source Sans 3\", serif",
                    "name": "Source Sans 3",
                    "slug": "source-sans-3"
                },
                {
                    "fontFace": [
                        {
                            "fontFamily": "Source Code Pro",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "200 900",
                            "src": [
                                "file:./assets/fonts/SourceCodePro-VariableFont_wght.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Source Code Pro",
                            "fontStretch": "normal",
                            "fontStyle": "italic",
                            "fontWeight": "200 900",
                            "src": [
                                "file:./assets/fonts/SourceCodePro-Italic-VariableFont_wght.ttf"
                            ]
                        }
                    ],
                    "fontFamily": "\"Source Code Pro\"",
                    "name": "Source Code Pro",
                    "slug": "source-code-pro"
                },
                {
                    "fontFace": [
                        {
                            "fontFamily": "Source Serif 4",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "200 900",
                            "src": [
                                "file:./assets/fonts/SourceSerif4-VariableFont_opsz,wght.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Source Serif 4",
                            "fontStretch": "normal",
                            "fontStyle": "italic",
                            "fontWeight": "200 900",
                            "src": [
                                "file:./assets/fonts/SourceSerif4-Italic-VariableFont_opsz,wght.ttf"
                            ]
                        }
                    ],
                    "fontFamily": "\"Source Serif 4\"",
                    "name": "Source Serif 4",
                    "slug": "source-serif-4"
                }
            ]
