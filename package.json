{
  "name": "ariom",
  "version": "1.0.0",
  "description": "A custom child theme",
  "main": "index.js",
  "scripts": {
    "dev:css": "npx tailwindcss -i ./_src/css/ariom.css -o ./css/ariom.css",
    "dev:js": "node _esbuild/build.config.mjs",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:js\"",
    "watch:css": "npx tailwindcss -i ./_src/css/ariom.css -o ./css/ariom.css --watch",
    "watch:js": "node _esbuild/watch.config.mjs --watch",
    "watch": "concurrently \"npm run watch:css\" \"npm run watch:js\"",
    "sync": "browser-sync start --proxy \"$npm_config_hostname\"  --host \"$npm_config_hostname\" --no-inject-changes --files=\"./\"",
    "watch:sync": "concurrently \"npm run watch\" \"npm run sync\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "autoprefixer": "^10.4.16",
    "browser-sync": "^3.0.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "daisyui": "^4.12.10",
    "esbuild": "^0.19.11",
    "postcss": "^8.4.32",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-nested-ancestors": "^3.0.0",
    "resolve-url-loader": "^5.0.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-debug-screens": "^2.2.1"
  }
}
